{
  "name": "browser-hrtime",
  "version": "1.1.7",
  "description": "use process.hrtime() in the browser",
  "main": "dist/hrtime.js",
  "typings": "dist/hrtime.d.ts",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "size": "size-limit",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest && nyc report --reporter=lcov && npm run size",
    "testnew": "jest && nyc merge .nyc_output .nyc_output/out.json && nyc report --reporter=lcov",
    "lint": "tslint -p tsconfig.json",
    "semantic-release": "semantic-release"
  },
  "size-limit": [
    {
      "limit": "1 KB",
      "path": "dist/hrtime.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinjs/browser-hrtime.git"
  },
  "keywords": [
    "hrtime",
    "process",
    "perfomance",
    "angular",
    "node"
  ],
  "author": "Vlad Tansky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinjs/browser-hrtime/issues"
  },
  "homepage": "https://github.com/cabinjs/browser-hrtime#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "coveralls": "^3.1.0",
    "dotenv-cli": "^4.0.0",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "puppeteer": "^8.0.0",
    "puppeteer-to-istanbul": "^1.4.0",
    "rollup": "^2.41.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.2",
    "size-limit": "^4.9.2",
    "terser": "^5.6.0",
    "ts-jest": "^26.5.5",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
