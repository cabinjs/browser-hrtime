{
  "name": "browser-hrtime",
  "version": "1.1.6",
  "description": "use process.hrtime() in the browser",
  "main": "lib/hrtime.js",
  "typings": "lib/hrtime.d.ts",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest && nyc report --reporter=lcov",
    "testnew": "jest && nyc merge .nyc_output .nyc_output/out.json && nyc report --reporter=lcov",
    "lint": "tslint -p tsconfig.json",
    "release": "dotenv release-it --no-npm.publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltansky/browser-hrtime.git"
  },
  "keywords": [
    "hrtime",
    "process",
    "perfomance",
    "angular",
    "node"
  ],
  "author": "Vlad Tansky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vltansky/browser-hrtime/issues"
  },
  "homepage": "https://github.com/vltansky/browser-hrtime#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.12",
    "coveralls": "^3.1.0",
    "dotenv-cli": "^3.1.0",
    "http-server": "^0.12.3",
    "jest": "^25.5.4",
    "jest-puppeteer": "^4.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.4",
    "puppeteer": "^3.3.0",
    "puppeteer-to-istanbul": "^1.4.0",
    "release-it": "^13.6.3",
    "rollup": "^2.16.1",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "terser": "^4.7.0",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "files": [
    "lib"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "hooks": {
      "before:init": "npm test"
    }
  },
  "dependencies": {}
}
