{
  "name": "browser-hrtime",
  "version": "1.0.4",
  "description": "use process.hrtime() in the browser",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "browser": "lib/index.es.js",
  "scripts": {
    "test": "jest && nyc report --reporter=lcov",
    "build": "rollup -c && tsc",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "release": "dotenv release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltansky/browser-hrtime.git"
  },
  "keywords": [
    "hrtime",
    "process",
    "perfomance",
    "angular",
    "node"
  ],
  "author": "Vlad Tansky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vltansky/browser-hrtime/issues"
  },
  "homepage": "https://github.com/vltansky/browser-hrtime#readme",
  "devDependencies": {
    "@types/node": "^13.13.0",
    "coveralls": "^3.1.0",
    "dotenv-cli": "^3.1.0",
    "jest": "^25.4.0",
    "jest-puppeteer": "^4.4.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "puppeteer": "^3.0.1",
    "puppeteer-to-istanbul": "^1.2.2",
    "release-it": "^13.5.5",
    "rollup": "^2.7.3",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "terser": "^4.6.12",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib"
  ],
  "release-it": {
    "github": {
      "release": true
    }
  },
  "dependencies": {}
}
