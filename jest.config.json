{
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testMatch": [
        "**/test/**/*.(js|ts)"
    ],
    "coverageReporters": ["text", "lcov", "json", "json-summary"],
    "preset": "jest-puppeteer",
    "verbose": true,
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }