{
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testMatch": [
        "**/test/**/*.(spec|test).(js|ts)"
    ],
    "collectCoverage": false,
    "coverageDirectory": ".nyc_output",
    "coverageReporters": ["json-summary", "lcov"],
    "testEnvironment": "jsdom",
    "preset": "jest-puppeteer",
    "verbose": true,
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }